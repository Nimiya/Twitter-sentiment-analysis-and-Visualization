library(readr)
pos <- read_csv("C:/Python34/twitter sentiment analysis/pos.csv")
p=nrow(pos)
library(readr)
neg <- read_csv("C:/Python34/twitter sentiment analysis/neg.csv")
n=nrow(neg)
library(readr)
neu <- read_csv("C:/Python34/twitter sentiment analysis/neu.csv")
t=nrow(neu)
x<- c(p,n,t)
labels<-c("Positive","Negative","Neutral")
pie3D(x,labels = labels,explode = 0.1, main = "Twitter Sentiment Analysis - #newTweets")
